# === Advanced Strategy Mode Overrides ===
# Advanced strategy configuration with aggressive risk parameters

# === STRATEGY MODE ===
STRATEGY_MODE=advanced
PERFORMANCE_MODE=ultra_high

# === ADVANCED STRATEGY PARAMETERS ===
TARGET_WEEKLY_RETURN=0.10
BASE_RISK_PER_TRADE=0.05
MAX_RISK_PER_TRADE=0.15
MIN_QUALITY_SCORE=70
MIN_CONFIDENCE_SCORE=0.9
POSITION_SIZE_PCT=20.0
CONFIDENCE_MULTIPLIER=2.0

# === ULTRA HIGH PERFORMANCE SETTINGS ===
POLL_SECONDS=0.5
BATCH_SIZE=20
CACHE_TTL_SECONDS=30
MAX_WORKERS=8

# === ADVANCED RISK MANAGEMENT ===
MAX_OPEN_POSITIONS=8
STOP_LOSS_ATR_MULTIPLIER=1.8
TAKE_PROFIT_ATR_MULTIPLIER=3.0
MAX_DAILY_TRADES=20

# === ADVANCED DATA USAGE ===
# Rev 00236 (Jan 9, 2026): DATA_PRIORITY deprecated - use BROKER_TYPE in data-providers.env
# All data comes from configured broker (BROKER_TYPE) - no third-party fallback
# DATA_PRIORITY=etrade,polygon,yfinance  # DEPRECATED - use BROKER_TYPE instead
# POLYGON_QPM_BUDGET=500  # DEPRECATED - broker data only
# ALPHA_VANTAGE_DAILY_LIMIT=800  # DEPRECATED - broker data only

# === ADVANCED LOGGING ===
LOG_PATH=logs/advanced_strategy.log
SESSION_LOG_LEVEL=INFO

# === ADVANCED ALERTING ===
ALERT_ON_ENTRY_SIGNALS=true
ALERT_ON_EXIT_SIGNALS=true
ALERT_ON_ERRORS=true
ALERT_ON_PERFORMANCE_ISSUES=true
ALERT_ON_DATA_FAILURES=true
